{
  "manifest_version": 3,
  "name": "Focus Fish Tank",
  "version": "0.1.0",
  "description": "Stay focused or your fish will suffer. A virtual fish tank that reacts to your browsing habits.",
  "permissions": ["tabs", "storage", "alarms"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_title": "Focus Fish Tank",
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}
